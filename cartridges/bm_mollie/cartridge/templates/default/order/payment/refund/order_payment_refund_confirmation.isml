<isdecorate template="pt_csc.isml">
    <isif condition="${pdict.success}">
        <div class="alert alert-success">
            Successfully refunded
        </div>
    <iselse/>
        <div class="alert alert-danger">
            Refund failed. <isprint value="${pdict.errorMessage}" />.
        </div>
    </isif>
    <hr />
</isdecorate>
