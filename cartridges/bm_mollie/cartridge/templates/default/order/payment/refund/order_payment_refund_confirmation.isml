<isdecorate template="pt_csc.isml">
    <isif condition="${pdict.success}">
        <div class="alert alert-success">
            ${Resource.msg('label.refund.success', 'bm', null)}
        </div>
    <iselse/>
        <div class="alert alert-danger">
            ${Resource.msg('label.refund.failed', 'bm', null)} <isprint value="${pdict.errorMessage}" />
        </div>
    </isif>
    <hr />
</isdecorate>
