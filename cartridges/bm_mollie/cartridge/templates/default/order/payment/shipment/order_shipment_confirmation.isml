<isdecorate template="pt_csc.isml">
    <isif condition="${pdict.success}">
        <div class="alert alert-success">
            ${Resource.msg('label.shipment.success', 'bm', null)}
        </div>
    <iselse/>
        <div class="alert alert-danger">
            ${Resource.msg('label.shipment.failed', 'bm', null)} <isprint value="${pdict.message}" />
        </div>
    </isif>
</isdecorate>
