<isdecorate template="pt_csc.isml">
    <isif condition="${pdict.success}">
        <div class="alert alert-success">
            Successfully processed.
        </div>
    <iselse>
        <div class="alert alert-danger">
            Process failed. <isprint value="${pdict.message}">.
        </div>
    </isif>
</isdecorate>
